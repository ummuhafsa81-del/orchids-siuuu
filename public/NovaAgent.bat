@echo off
setlocal enabledelayedexpansion
title Nova Local Agent
color 0A
echo.
echo ========================================
echo    NOVA LOCAL AGENT - Starting...
echo ========================================
echo.

set "TEMPPS=%TEMP%\NovaAgent.ps1"

:: Use PowerShell to read this batch file and extract the base64 between markers
powershell -Command "$f=[System.IO.File]::ReadAllText('%~f0'); $s1='BASE64'+'_START'; $s2='BASE64'+'_END'; $p1=$f.IndexOf($s1)+$s1.Length; $p2=$f.LastIndexOf($s2); $b64=$f.Substring($p1,$p2-$p1).Trim() -replace '\s',''; [System.IO.File]::WriteAllText('%TEMPPS%', [System.Text.Encoding]::UTF8.GetString([System.Convert]::FromBase64String($b64)))"

if not exist "%TEMPPS%" (
    echo [ERROR] Failed to extract agent script.
    pause
    exit /b 1
)

powershell -ExecutionPolicy Bypass -File "%TEMPPS%"

del "%TEMPPS%" 2>nul
echo.
echo Agent stopped.
pause
goto :eof

BASE64_START

BASE64_END
